{
  "name": "@i-xi-dev/bytes",
  "version": "0.0.23",
  "description": "A JavaScript byte array library for the browser, Deno and Node.js",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "node": "./node/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build && webpack",
    "clean": "tsc --build --clean",
    "lint": "eslint --ext ts --fix ./src/",
    "test": "mocha test/node/ --recursive",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/i-xi-dev/bytes.es.git"
  },
  "keywords": [
    "base64",
    "base64url",
    "blob",
    "byte",
    "bytes",
    "dataurl",
    "integrity",
    "sha256",
    "sha384",
    "sha512",
    "utf-8",
    "browser",
    "deno",
    "nodejs"
  ],
  "author": "i-xi-dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/i-xi-dev/bytes.es/issues"
  },
  "homepage": "https://github.com/i-xi-dev/bytes.es#readme",
  "files": [
    "dist",
    "node"
  ],
  "engines": {
    "node": ">=16.5.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "iconv-lite": "^0.6.3",
    "mocha": "^9.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@i-xi-dev/base64": "^1.0.0"
  }
}
